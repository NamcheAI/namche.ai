# Bind for the API proxy (nginx should proxy to this service)
HOST=0.0.0.0
PORT=8787

# Global config path override (optional)
# NAMCHE_PROXY_CONFIG=/Users/<user>/namche.ai/api-proxy/routes.config.json

# Ingress secrets expected by api.namche.ai
WEBHOOK_SECRET_TASHI_IN=
WEBHOOK_SECRET_NIMA_IN=
WEBHOOK_SECRET_PEMA_IN=

# Secrets forwarded from api.namche.ai to agent machines over Tailscale
WEBHOOK_SECRET_TASHI_OUT=
WEBHOOK_SECRET_NIMA_OUT=
WEBHOOK_SECRET_PEMA_OUT=
