---
import Base from '../layouts/Base.astro';
import { resolveSiteId, SITES } from '../sites';

const siteId = resolveSiteId();
const { meta, component: SitePage } = SITES[siteId];
---

<Base title={meta.title} description={meta.description} brandLabel={meta.brandLabel} siteId={siteId}>
  <SitePage />
</Base>
